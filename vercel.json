{
    "version":2,
    "builds":[{
        "src":"myportfolio/wsgi.py",
        "use":"@vercel/python",
        "config":{"maxLambdaSize":"15mb","runtime":"python3.9"}
    }

    

],
    "routes":[{
        "src":"/(.*)",
        "dest":"myportfolio/wsgi.py"
    },
{
    "src":"/static/(.*)",
    "dest":"/static/$1"
}]
}