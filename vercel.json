{
    "builds":[{
        "src":"manage.py",
        "use":"@vercel/python",
        "config":{"maxLambdaSize":"15mb","runtime":"python"}
    },{
        "src":"build_files.sh",
        "use":"@vercel/static-build",
        "config":{"distDir":"staticfiles_build"}
    }],
    "routes":[{
        "src":"/(.*)",
        "dest":"/manage.py"
    },
{
    "src":"/static/(.*)",
    "dest":"/static/$1"
}]
}